/**
 * node_push - add node to de list
 * @stack: stack
 * @line_number: number of line
 */

void node_push(stack_t **stack, unsigned int line_number)
{
	stack_t *head;
	(void)line_number;
	head = malloc(sizeof(stack_t));
	if (head == NULL)
	{
		exit(EXIT_FAILURE);
	}

	head->n = num;
	head->next = *stack;
	head->prev = NULL;
	if (*stack != NULL)
		(*stack)->prev = top;
	*stack = head;
}
